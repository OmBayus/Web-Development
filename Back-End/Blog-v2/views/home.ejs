<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%= StartingContent %></p>

<% posts.forEach(function(post){ %>
  <% var link = "/posts/" + post.title %>
  <h2> <%= post.title %> </h2>
  <p> <%= post.content.substring(0,100) + "..."%> <a href="<%= link %>">Read More</a></p>

<%})%>

<%- include('partials/footer'); -%>
